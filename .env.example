# GitHub source update (private repo)
SYSTEM_UPDATE_GITHUB_ENABLED=true
SYSTEM_UPDATE_GITHUB_OWNER=
SYSTEM_UPDATE_GITHUB_REPO=
SYSTEM_UPDATE_GITHUB_BRANCH=main
SYSTEM_UPDATE_GITHUB_TOKEN=
SYSTEM_UPDATE_GITHUB_RELEASE_TAG=panel-main-latest
SYSTEM_UPDATE_GITHUB_RELEASE_ASSET=update-package.zip
