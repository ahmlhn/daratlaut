SYSTEM_UPDATE_ALLOW_ROLES=owner,admin
SYSTEM_UPDATE_MAX_MB=300
SYSTEM_UPDATE_CHUNK_SIZE=200
SYSTEM_UPDATE_ALLOW_DOWNLOAD=false
